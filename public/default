server {
    listen 80;
    server_name 217.151.230.35;
    root /var/www/CTFd;

    location / {
        proxy_pass http://127.0.0.1:8888;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }
}


server {
    listen 199;
    server_name 217.151.230.35;

    root /var/www/cyberkol/;

    client_max_body_size 600M;

    location / {
        proxy_pass http://127.0.0.1:1909;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }

    location /media/ {
        alias /var/www/cyberkol/media/;
    }

    location /static/ {
        alias /var/www/cyberkol/static/;
    }
}

server {
    listen 200;
    server_name 217.151.230.35;
    
    root /var/www/kia/;
    
    client_max_body_size 600M;

    location / {
        proxy_pass http://127.0.0.1:2000;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }
    
    location /static/ {
        alias /var/www/kia/static/;
    }
    
}



server {
    listen 1000;
    server_name 217.151.230.35;
    root /var/www/it-fest;

    location / {
        index index.html;
        try_files $uri $uri/ =404;
    }

    # Дополнительно можно указать типы содержимого
    types {
        text/html html;
        text/css css;
        image/jpeg jpeg jpg;
        image/png png;
	image/svg+xml svg;
    }
}

